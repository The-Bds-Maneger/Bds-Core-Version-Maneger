{
  "name": "@the-bds-maneger/server_versions",
  "version": "2.0.2",
  "description": "Auto Find Minecraft Server Versions",
  "homepage": "https://github.com/The-Bds-Maneger/ServerVersions#readme",
  "private": false,
  "author": "The Bds Maneger Project Authors",
  "license": "MIT",
  "types": "./dist/cjs/index.d.ts",
  "main": "src/cjs/index.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/cjs/index.js"
  },
  "scripts": {
    "searchVersion": "ts-node src/fetchVersion/index.ts",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "search": "ts-node src/fetchVersion/index.ts",
    "build": "npm run build:cjs && npm run build:esm",
    "changelog": "auto-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Bds-Maneger/ServerVersions.git"
  },
  "keywords": [
    "Minecraft",
    "Server",
    "automatic"
  ],
  "bugs": {
    "url": "https://github.com/The-Bds-Maneger/ServerVersions/issues"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsdom": "^19.0.0",
    "mongoose": "^6.2.10",
    "typescript": "^4.6.3",
    "undici": "^5.1.1",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/cli-color": "^2.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/yargs": "^17.0.10",
    "adm-zip": "^0.5.9",
    "auto-changelog": "^2.4.0",
    "cli-color": "^2.0.2",
    "ts-node": "^10.7.0"
  }
}
